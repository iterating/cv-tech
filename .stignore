# (?d) prefix makes Syncthing delete files that match the pattern
# Patterns without (?d) will be ignored by Git but kept by Syncthing

###################
# Sync & Version Control
###################

# Sync conflict prevention
(?d)**/*sync-conflict*
(?d)**/*.sync-conflict*
(?d)**/*conflicted\ copy*
(?d)**.cursor-positions.sync-conflict*
(?d)**/workspace.sync-conflict*
(?d)**/.git/*sync-conflict*

# Git - preserve ignore files but clean git internals
(?d).git/
!.git/config
!.git/HEAD
!.git/refs/heads/main
!.git/refs/heads/master
!**/.gitignore
.gitignore.bak
.gitignore.tmp
.gitignore.old

# Syncthing - preserve ignore files but clean syncthing internals
(?d).stversions/
(?d).SyncTemp/
(?d).SyncArchive/
(?d)**/*.SyncPart
(?d)**/*.SyncTemp
(?d)**/*.SyncOld
(?d).stfolder/
(?d).SyncID
(?d).sync/
!**/.stignore
.stignore.bak
.stignore.tmp
.stignore.old

